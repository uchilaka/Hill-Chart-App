{
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {"jsx": true},
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["react"],
  "rules": {
    "quotes": ["warn", "single"],
    "no-unused-expressions": 0,
    "no-cond-assign": 0,
    "no-control-rejex": 0,
    "no-prototype-builtins": 0,
    "no-setter-return": 0
  }
}